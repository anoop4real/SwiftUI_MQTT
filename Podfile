# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SwiftUI_MQTT' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftUI_MQTT
    pod 'CocoaMQTT'

  target 'SwiftUI_MQTTTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'CocoaMQTT'
  end

  target 'SwiftUI_MQTTUITests' do
    # Pods for testing
    pod 'CocoaMQTT'
  end

end
